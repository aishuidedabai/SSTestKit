source 'https://github.com/CocoaPods/Specs.git'
use_frameworks!

abstract_target 'iOS' do
    platform :ios, '11.0'
    
    target 'SSKit' do
        pod 'YYModel'
        pod 'MMWormhole', '~> 2.0.0'
    end
    
    
    target 'PacketProcessor' do
        pod 'CocoaAsyncSocket', '~> 7.4.3'
    end
    
    
    target 'SSTestKit' do
        pod 'YYModel'
        pod 'MMWormhole', '~> 2.0.0'
    end
    
    
    target 'Tunnel' do
        pod 'CocoaAsyncSocket', '~> 7.4.3'
        pod 'YYModel'
        pod 'MMWormhole', '~> 2.0.0'
    end
    
end


